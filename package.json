{
  "name": "digify",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "npm run dev",
    "build": "nuxt build",
    "build:watch": "nuxt build --watch",
    "dev": "dotenv -e .env.dev nuxt dev --o",
    "build:css": "node scripts/sass/compile-sass.js",
    "build:css:vite": "vite build",
    "generate": "nuxt generate",
    "generate:dev": "nuxt --dotenv .env.dev generate ",
    "generate:prod": "dotenv -e .env.prod nuxt generate ",
    "generate:gh": "dotenv -e .env.prod.gh nuxt generate",
    "generate:netilfy": "dotenv -e .env.prod.netilfy nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "gh-publish": "gh-pages -d dist",
    "predeploy:": "npm run generate",
    "deploy": "npm run  gh-publish",
    "deploy:gh": "npm run generate:gh && npm run deploy",
    "deploy:netilfy": "npm run generate:netilfy && npm run deploy",
    "preversion": "npm run deploy:netilfy",
    "postversion": "git push && git push --tags",
    "build:bootstrap": "postcss assets/sass/bootstrap.scss -o assest/css/bootstrap.css"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@types/node": "^18.17.1",
    "@types/postcss-rtl": "^2.0.0",
    "autoprefixer": "^10.4.20",
    "css-loader": "^7.1.2",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.2",
    "gh-pages": "^6.0.0",
    "nuxt": "^3.6.5",
    "nuxt-gtag": "^3.0.1",
    "postcss": "^8.4.47",
    "postcss-at-rules-variables": "^0.3.0",
    "postcss-cli": "^11.0.0",
    "postcss-conditionals": "^2.1.0",
    "postcss-rtl": "^2.0.0",
    "postcss-rtlcss": "^5.5.0",
    "rtlcss": "^4.3.0",
    "sass": "^1.64.2",
    "sass-loader": "^10.4.1",
    "vite-plugin-vsharp": "^1.4.2",
    "vite-plugin-vuetify": "^2.0.4"
  },
  "dependencies": {
    "@emailjs/browser": "^3.12.1",
    "@mdi/font": "^7.4.47",
    "@nuxtjs/google-analytics": "^2.4.0",
    "@nuxtjs/i18n": "^8.5.5",
    "@nuxtjs/robots": "^3.0.0",
    "@pinia/nuxt": "^0.4.11",
    "@types/rtlcss": "^3.5.4",
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.3",
    "animate.css": "^4.1.1",
    "aos": "^2.3.4",
    "axios": "^1.7.7",
    "bootstrap": "^5.3.1",
    "click-outside": "^2.0.2",
    "email-js": "^2.0.2",
    "emailjs-com": "^3.2.0",
    "i18next": "^23.15.1",
    "i18next-browser-languagedetector": "^8.0.0",
    "i18next-http-backend": "^2.6.1",
    "lodash": "^4.17.21",
    "normalize.css": "^8.0.1",
    "pinia": "^2.1.6",
    "swiper": "^10.1.0",
    "vue-gtag-next": "^1.14.0",
    "vue-i18next": "^0.9.4",
    "vue-toast-notification": "^3.1.1",
    "vuetify": "^3.7.2"
  },
  "version": "3.2.0",
  "overrides": {
    "vue": "latest"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
